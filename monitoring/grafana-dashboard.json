{
  "dashboard": {
    "id": null,
    "title": "Prism AI Monitoring",
    "tags": ["prism-ai", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Request Rate",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(prism_ai_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} {{status}}"
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Request Duration",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(prism_ai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(prism_ai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Requests",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(prism_ai_requests_total)",
            "instant": true
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Error Rate",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(prism_ai_requests_total{status=~\"5..\"}[5m])",
            "instant": true
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "id": 5,
        "type": "graph",
        "title": "System Memory",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 4
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory Usage"
          }
        ],
        "datasource": "Prometheus"
      }
    ]
  }
}